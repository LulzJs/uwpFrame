<ResourceDictionary x:Class="UWP.LIB.Controls.TextBox.TextBoxTemplate"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:textBox="using:UWP.LIB.Controls.TextBox">
    <DataTemplate x:Key="ApiText" x:DataType="textBox:TextBoxViewModel">
        <TextBox Text="{x:Bind Text}" />
    </DataTemplate>
</ResourceDictionary>
